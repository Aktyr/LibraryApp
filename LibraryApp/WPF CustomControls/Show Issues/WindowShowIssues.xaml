<Window x:Class="LibraryApp.WPF_CustomControls.Show_Issues.WindowShowIssues"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LibraryApp.WPF_CustomControls.Show_Issues"
        mc:Ignorable="d"
        Title="WindowShowIssues" Height="150" Width="400">
    <Grid>


        <StackPanel Grid.Column="0" Margin="20,20,20,20" Visibility="Visible" >

            <Button HorizontalAlignment="Stretch" Margin="0,5" Padding="0, 5"
                    Content="Issed Books" Click="Button_Click_IssedBooks"/>
            
            <Button HorizontalAlignment="Stretch" Margin="0,5" Padding="0, 5"
                    Content="User Books" Click="Button_Click_Users"/>


        </StackPanel>

    </Grid>
</Window>
